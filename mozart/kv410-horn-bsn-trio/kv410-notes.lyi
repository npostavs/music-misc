    f2 r4 \bar "|."
}

Bassoon = \relative e' {
    \settings \key c \major e8 -. -\p g,8 -. | % 1
    f'8 -. g,8 -. e'8 -. g,8 -. b8 -. g8 -. c8 -. g8 -. | % 2
    d'8 -. a8 -. b8 -. g8 -. c8 ( b8 a8 g8 ) | % 3
    f8 ( e8 f8-\cresc g8 a8 b8 c8 fs,8 ) | % 4
    g8-\f ( f?8 e8 d8 ) c8-\! -\p c'8 ( b8 c8 ) | % 5
    f,8 -. c'8 -. g8 -. c8 -. a8 -. c8 -. e,8 -. c'8 -. | % 6
    f,8 ( a8 c8 f8 ) e8 ( g,8 ef'8 g,8 ) | % 7
    d'8 ( g,8 b8-\cresc g8 c8 g8 fs'8 g,8 ) | % 8
    g'8-\! -\p g,8 ( b8 c8 ) b8 ( g8 ) e8 ( c8 ) | % 9
    f?8 ( e8 f8 af8 g8 fs8 g8 bf8 ) | \barNumberCheck #10
    a?8 ( f?8 c'8 bf8 a8 c8 ) f,8 -. f'8 -. | % 11
    e8 ( d8 cs8 a8 ) d8 ( c8 b8 g8 ) | % 12
    c8-\cresc ( d8 e8 f8 g4. f8 ) -\p | % 13
    e8 ( g,8 ) c8 ( g8 ) f'8 ( g,8 ) d'8 ( g,8 ) | % 14
    c8-\cresc ( d8 e8 d8 f8 g8 a8 f8 ) | % 15
    e8 ( d8 c8 -\p b8 c8 b8 a8 f8 ) | % 16
    f'8 ( d8 ) e8 ( c8 ) d8 ( a8 ) b8 ( g8 ) | % 17
    c4 ( f,4 ) g8 -! e'8 ( f8 d8 ) | % 18
    c8 ( b8 c8 a8 ) fs8 -! fs'8 ( e8 d8 ) | % 19
    g4 g,4 c4 f,4 | \barNumberCheck #20
    g8 ( f8 e8 c8 ) g'8 -! d'8 ( g8 f8 ) | % 21
    e2-\cresc d4 c4 | % 22
    f4 -\p e4 d8 ( e8 f8 g8 | % 23
    a2 ) r4 d,8 ( -\f c8 ) | % 24
    b4 ( -\p c4 d4 ) d,4 | % 25
    e2-\cresc ( f2 ) | % 26
    g4 -\p a4 f4 g4 | % 27
    c,2 r4 \bar "|."
}
