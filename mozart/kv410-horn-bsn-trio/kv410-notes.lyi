    b4. b8 c2 ~ | % 5
    c1 ~ | % 6
    c2. g'4 ( -\p | % 7
    fs4 f4 -\cresc e4 ef4 ) | % 8
    d4. ( -\p ef8 ) d4 r4 | % 9
    r4 c4 c'2 ~ | \barNumberCheck #10
    c2. ( a4 ) | % 11
    g4 ( e4 f4 d4 ) | % 12
    e8 ( -\cresc d8 c8 d8 ) b8 a8 g4 ~ | % 13
    g4 c2 ( -\p b4 ) | % 14
    c2. f8. ( g32 a32 ) | % 15
    g8 ( f8 e8 d8 e4 f4 ) | % 16
    b,4 ( -\fp c4 ) f2 | % 17
    e8 ( f8 g8 a8 ) c,4 ( b4 ) | % 18
    f'4 ( -\fp e4 ) a4. ( b16 c16 ) | % 19
    c8 ( b8 a8 g8 f8 e8 d8 c8 ) | \barNumberCheck #20
    b4 ( -\fp c4 ) d4 r4 | % 21
    r4 g,8-\cresc ( a8 b8 c8 d8 e8 ) | % 22
    f8 c8 ( -\p d8 e8 f8 g8 a8 b8 ) | % 23
    c8 ( b8 a8 -\cresc g8 f8 e8 d4 ) ~ | % 24
